            url="http://www.eclipse.org/modeling/updates/"/>
        <repository
            url="https://bitub.github.io/step-p2-antlr4/p2/"/>
        <repository
            url="https://bitub.github.io/step-p2-stringconverter/p2/"/>
        <repository
            url="https://bitub.github.io/step-ifc4"/>
        <repository
            url="http://download.eclipse.org/viatra/updates/release"/>
      </repositoryList>
    </targlet>
  </setupTask>
  <setupTask
      xsi:type="projects:ProjectsBuildTask"
      onlyNewProjects="true"/>
  <setupTask
      xsi:type="launching:LaunchTask"
      launcher="Generate Language Infrastructure (de.bitub.step.express)"/>
  <setupTask
      xsi:type="launching:LaunchTask"
      launcher="GenerateExpressionStrings.mwe2"/>
  <setupTask
      xsi:type="launching:LaunchTask"
      launcher="GenerateTextualMVD.mwe2"/>
  <setupTask
      xsi:type="setup:CompoundTask"
      name="Mylyn Github Issues">
    <setupTask
        xsi:type="mylyn:MylynQueriesTask"
        id="github.issues"
        connectorKind="github"
        repositoryURL="https://github.com/bitub/step"
        userID="${github.user.id}"
        password="">
      <query
          summary="STEP Issues"
          url="">
        <attribute
            key="state"
            value="open::"/>
      </query>
    </setupTask>
  </setupTask>
  <setupTask
      xsi:type="setup.workingsets:WorkingSetTask"
      id="workingsets">
    <workingSet
        name="P21 parser">
      <predicate
          xsi:type="predicates:AndPredicate">
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*\.p21(\..*)?"/>
      </predicate>
    </workingSet>
    <workingSet
        name="EXPRESS to Xcore M2M">
      <predicate
          xsi:type="predicates:AndPredicate">
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*\.express(\..*)?"/>
      </predicate>
    </workingSet>
    <workingSet
        name="Release Engineering"
        id="">
      <predicate
          xsi:type="predicates:OrPredicate">
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*\.step-updatesite"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*\.build-resources"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*\.target-definition"/>
      </predicate>
    </workingSet>
    <workingSet
        name="MVD &amp; tMVD">
      <predicate
          xsi:type="predicates:AndPredicate">
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*\.mvd(\..*)?"/>
      </predicate>
    </workingSet>
    <description>The dynamic working sets for ${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="projects:ProjectsBuildTask"
      onlyNewProjects="true"/>
  <stream name="master"
      label="master">
    <setupTask
        xsi:type="setup:VariableTask"
        name="target.generate.versions"
        value="0.1.0-SNAPSHOT"
        defaultValue="0.1.0-SNAPSHOT"/>
    <setupTask
        xsi:type="setup:VariableTask"
        name="eclipse.target.platform"
        value="Neon"
        defaultValue="Neon"
        storageURI="scope://Workspace"
        label="Target Platform">
      <description>Choose the compatibility level of the target platform</description>
    </setupTask>
  </stream>
  <logicalProjectContainer
      xsi:type="setup:ProjectCatalog"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='com.github']"/>
  <description>M2M for EXPRESS to Java using Xtext and Xcore.</description>
</setup:Project>
