                                </AttributeRule>
                                <AttributeRule RuleID="Identifier" AttributeName="RepresentationIdentifier" >
                                  <EntityRules>
                                    <EntityRule EntityName="IfcLabel"  />
                                  </EntityRules>
                                </AttributeRule>
